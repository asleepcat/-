<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.bme.base.model">

	<class name="FeelRecode" table="t_feelrecode">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="useid" />
		<property name="balance" />
		<property name="fee" />
		<property name="operator" />
		<property name="operatordate" type="timestamp" />

	</class>

</hibernate-mapping>
